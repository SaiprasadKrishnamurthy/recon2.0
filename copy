#!/bin/bash

cp ./reconciliation-engine/target/manifests/k8s/reconciliation-engine/nats-deployment.yml ./manifests/
cp ./reconciliation-engine/target/manifests/k8s/reconciliation-engine/configMap_k8s.yml ./manifests/reconciliation_engine_configMap_k8s.yml
