cp ./reconciliation-engine-ui/target/manifests/k8s/reconciliation-engine-ui/configMap_k8s.yml ./manifests/reconciliation_engine_ui_configMap_k8s.yml
cp ./reconciliation-engine-ui/target/manifests/k8s/reconciliation-engine-ui/service-deployment_k8s.yml ./manifests/reconciliation_engine_ui_service_deployment_k8s.yml

